{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "module": "ES2020",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "plugins": [
      {
        "name": "typescript-plugin-css-modules"
      }
    ],
    "strictNullChecks": true,
    "target": "ES2020"
  }
}
