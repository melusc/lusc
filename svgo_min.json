{
  "multipass": true,
  "precision": 3,
  "plugins": [
    {
      "sortAttrs": {
        "order": [
          "id",
          "width",
          "height",
          "x",
          "x1",
          "x2",
          "y",
          "y1",
          "y2",
          "cx",
          "cy",
          "r",
          "fill",
          "stroke",
          "marker",
          "d",
          "points"
        ]
      }
    },
    { "removeScriptElement": true },
    { "removeDimensions": true },
    { "normalizeColorCase": true }
  ]
}
